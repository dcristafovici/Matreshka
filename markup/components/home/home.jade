include ../button/button
mixin home(data)
  section.home  
    .home-video
      video(muted="muted" preload="auto" height="auto" autoplay="autoplay" loop="loop")
        source(src='video.mp4' type="video/mp4")
    .content  
      .home-content
        .home-title  
          h1 matryoshka city
        .home-subtitle 
          span The first night club in Moscow City - theater 
        .home-worktime
          span Wed - Sat
          span 22:00 - 06:00
        .home-button
          +button('#', 'Бронь')
